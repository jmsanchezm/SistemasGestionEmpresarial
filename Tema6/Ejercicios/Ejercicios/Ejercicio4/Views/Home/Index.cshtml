@{
    ViewData["Title"] = "Home Page";
}


<style >
    table {
        border-collapse: collapse;
    }

    td {
        border: 1px solid black;
        padding: 5px;
        width: 400px;
        text-align: center;
    }
</style>

@{
    int result =  0 ;
    <table>

        @{
            for (int i = 1; i <= 10; i++)
            {
                <tr>
                    @{
                        for (int j = 1; j <= 10; j++)
                        {
                            result = i * j;
                            <td>@i x @j = @result</td>
                        }
                    }
                </tr>
            }
        }
       
    </table>
}